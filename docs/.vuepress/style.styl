@import './override';
    .imageFrame
        border-style: inset
        border-color: $accentColor
        border-width: 10px
        .imageInnerFrame
            border-style: outset
            border-color: $accentColor
            border-width: 10px
    .vaporWave
        animation: filter-animation 5s infinite;
    @keyframes filter-animation {
        0% {
            filter: sepia(0) saturate(2);
        }

        50% {
            filter: sepia(1) saturate(8);
        }

        100% {
            filter: sepia(0) saturate(2);
        }
    }